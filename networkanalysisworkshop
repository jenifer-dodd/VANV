//graph

=== OUR DATASET
[source, cypher]
----
CREATE
//People
(a:Person{name:'a'}),
(b:Person{name:'b'}),
(c:Person{name:'c'}),

//Places
(d:Location{name:'d'}),
(e:Location{name:'e'}),

//Relationships
(a)-[:R]->(b),
(b)-[:S]->(c),
(b)-[:T]->(d),
(c)-[:U]->(a),
(e)-[:Z]->(b)


----







